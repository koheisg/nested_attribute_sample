<p id="notice"><%= notice %></p>

<h1>Job Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Job</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_schedules.each do |job_schedule| %>
      <tr>
        <td><%= job_schedule.job %></td>
        <td><%= job_schedule.name %></td>
        <td><%= link_to 'Show', job_schedule %></td>
        <td><%= link_to 'Edit', edit_job_schedule_path(job_schedule) %></td>
        <td><%= link_to 'Destroy', job_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Schedule', new_job_schedule_path %>
